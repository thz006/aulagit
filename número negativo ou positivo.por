programa {
  funcao inicio() {
    //numero negativo ou positivo
    real num
    escreva("digite um número para saber se ele é positivo ou negativo: ")
    leia(num)
    se(num>0){
      escreva("este número é positivo")
    }
    se(num<0){
      escreva("este número é negativo")
    }
    se(num==0){
    escreva("0 não é positivo nem negativo")}

  }
}
